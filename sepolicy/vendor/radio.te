dontaudit {
    cnd
    netmgrd
    qti
    adpl
    hal_audio_default
    rild
    hal_imsrtp
    hal_rcsservice
    hal_sensors_default
    hal_graphics_composer_default
    sensors
    vendor_dpmd
} diag_device:chr_file { read write };
